{
  "realm": "event-pipeline",
  "enabled": true,
  "displayName": "Event Pipeline (Local)",
  "sslRequired": "none",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "rememberMe": true,
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Acesso padrao de usuario"
      },
      {
        "name": "admin",
        "description": "Acesso administrativo"
      }
    ]
  },
  "clients": [
    {
      "clientId": "user-web",
      "name": "User Web",
      "protocol": "openid-connect",
      "enabled": true,
      "publicClient": true,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": false,
      "redirectUris": [
        "http://localhost:5173/*"
      ],
      "webOrigins": [
        "http://localhost:5173"
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    },
    {
      "clientId": "admin-web",
      "name": "Admin Web",
      "protocol": "openid-connect",
      "enabled": true,
      "publicClient": true,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": false,
      "redirectUris": [
        "http://localhost:5174/*"
      ],
      "webOrigins": [
        "http://localhost:5174"
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    },
    {
      "clientId": "api-gateway",
      "name": "API Gateway",
      "protocol": "openid-connect",
      "enabled": true,
      "publicClient": false,
      "secret": "dev-api-gateway-secret",
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true
    }
  ],
  "users": [
    {
      "username": "demo-user",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Demo",
      "lastName": "User",
      "email": "demo-user@local.test",
      "credentials": [
        {
          "type": "password",
          "value": "demo123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "user"
      ]
    },
    {
      "username": "demo-admin",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Demo",
      "lastName": "Admin",
      "email": "demo-admin@local.test",
      "credentials": [
        {
          "type": "password",
          "value": "demo123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "user",
        "admin"
      ]
    }
  ]
}
