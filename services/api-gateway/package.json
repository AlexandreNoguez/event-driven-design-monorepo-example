{
  "name": "@event-pipeline/api-gateway",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prestart": "pnpm --filter @event-pipeline/shared build",
    "predev": "pnpm --filter @event-pipeline/shared build",
    "start": "pnpm exec nest start --entryFile services/api-gateway/src/main",
    "dev": "pnpm exec nest start --watch --entryFile services/api-gateway/src/main",
    "build": "pnpm exec nest build",
    "start:prod": "node dist/services/api-gateway/src/main.js",
    "typecheck": "pnpm exec tsc -p tsconfig.json --noEmit --incremental false",
    "lint": "pnpm exec eslint src"
  },
  "dependencies": {
    "@event-pipeline/shared": "workspace:*",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "amqplib": "^0.10.5",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "minio": "^8.0.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/jwk-to-pem": "^2.0.3"
  }
}
