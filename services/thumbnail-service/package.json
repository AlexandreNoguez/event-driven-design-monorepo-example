{
  "name": "@event-pipeline/thumbnail-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prestart": "pnpm --filter @event-pipeline/shared build",
    "predev": "pnpm --filter @event-pipeline/shared build",
    "start": "/bin/bash -lc 'set -a; [ -f ../../.env ] && source ../../.env; pnpm exec nest start --entryFile services/thumbnail-service/src/main'",
    "dev": "/bin/bash -lc 'set -a; [ -f ../../.env ] && source ../../.env; pnpm exec nest start --watch --entryFile services/thumbnail-service/src/main'",
    "build": "pnpm exec nest build",
    "start:prod": "node dist/services/thumbnail-service/src/main.js",
    "typecheck": "pnpm exec tsc -p tsconfig.json --noEmit",
    "lint": "pnpm exec eslint src"
  },
  "dependencies": {
    "@event-pipeline/shared": "workspace:*",
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "amqplib": "^0.10.5",
    "minio": "^8.0.6",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.5",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/pg": "^8.11.10"
  }
}
